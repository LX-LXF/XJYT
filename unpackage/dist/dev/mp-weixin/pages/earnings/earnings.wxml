<view><view class="charts"><ff-canvas vue-id="1" id="interval" canvas-id="interval" opts="{ opts }" bind:__l="__l"></ff-canvas></view><view class="{scrollTop>182?'topFix':'income'}"><view class="divLine"></view><view style="height:150rpx;padding-left:48rpx;"><view class="side"><view class="txt">矿机数量</view><view class="num">{{machine_num}}</view></view><view class="side2"><view class="txt">存储空间</view><view class="num">{{fs_cap+''}}<text class="fontStyle" space="true">T</text></view></view></view><view class="divLine"></view><view style="height:150rpx;padding-left:48rpx;"><view class="side"><view class="txt">本月收益</view><view class="num">{{month_profit+''}}<text class="fontStyle" space="true">FIL</text></view></view><view class="side2"><view class="txt">总计</view><view class="num">{{orders.user_total_profit+''}}<text class="fontStyle" space="true">FIL</text></view></view></view><view class="divLine"></view><view class="notes"><text class="top">收益记录</text><view class="line"><view class="select_box"><view class="select" catchtap="selectTap"><text class="select_text">{{selectData[index]}}</text><image class="select_img{selectShow&&select_img_rotate}" src="../../static/images/select.png" background-size="contain"></image></view><view class="option_box" style="height:{selectShow?(selectData.length>5?325:selectData.length*50):0}rpx;"><text class="option" wx:for="{selectData}" wx:key="this" data-index="{index}" catchtap="optionTap">{{item}}</text></view></view><view class="all">{{"收益："+month_profit+''}}<text class="fontStyle" space="true">FIL</text></view></view></view></view><scroll-view class="notes" scroll-y="true"><view class="none"><view class="list" wx:for="{orders.user_profit_data}" wx:key="id"><image class="pic" src="../../static/images/income.png"></image><view class="desc"><view class="info">收款</view><view class="info">{{item.trans_time}}</view></view><view class="add">{{"+"+item.num+"FIL"}}</view></view></view></scroll-view></view>